<h2 class="main">Routines</h2>
{{#paper-list}}
  {{#each model as |routine|}}
    {{#paper-item class="md-3-line"}}
      <div class="md-list-item-text">
        <h3>
          {{#link-to 'routines.show' routine}}{{routine.name}}{{/link-to}}
        </h3>
        <p>{{routine.sets.length}} sets</p>
      </div>
    {{/paper-item}}
  {{/each}}
{{/paper-list}}

<h3>{{#link-to 'routines.new'}}Add Routine{{/link-to}}</h3>
{{outlet}}
